<h2>Add new Product</h2>
<form id="idForm">
    <label for="title">Title</label>
    <input type="text" required name="title" id="title">
    <label for="description">Description</label>
    <input type="text" required name="description" id="description">
    <label for="price">Price</label>
    <input type="number" required name="price" id="price">
    <label for="thumbnail">Thumbnail</label>
    <input type="text" required name="thumbnail" id="thumbnail">
    <label for="code">Code</label>
    <input type="text" required name="code" id="code">
    <label for="stock">Stock</label>
    <input type="number" required name="stock" id="stock">
    <button type="submit">Submit</button>
</form>

<br><br>

<h2>Products List</h2>
<table id="productsTable">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Description</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>